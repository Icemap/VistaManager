<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_lon_lat_select"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wqz.vistamanager.LonLatSelectActivity">

    <com.wqz.view.TitleBar
        android:id="@+id/select_lonlat_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

    <com.baidu.mapapi.map.MapView
        android:id="@+id/mv_select_lonlat"
        android:layout_below="@+id/select_lonlat_title_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true" />

</RelativeLayout>
